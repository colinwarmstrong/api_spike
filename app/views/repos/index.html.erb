<h2>Repos</h2>

<% @repos.each do |repo| %>
  <div class='repo'>
    <h4><%= repo.name %></h4>
    <p><%= repo.description %></p>
    <p>Main Language: <%= repo.language %> </p>
    <p>Forks: <%= repo.forks_count %> </p>
    <p>Last Updated On <%= repo.updated_at.strftime('%m/%d/%Y') %>  
  </div>
<% end %>
